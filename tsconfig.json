{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "Api/*": [
        "./app/api/*"
      ],
      "Component/*": [
        "./app/component/*"
      ],
      "Constants/*": [
        "./app/constants/*"
      ],
      "Hooks/*": [
        "./app/hooks/*"
      ],
      "Route/*": [
        "./app/route/*"
      ],
      "Style/*": [
        "./app/style/*"
      ],
      "Type/*": [
        "./app/type/*"
      ],
      "Util/*": [
        "./app/util/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
