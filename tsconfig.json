{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "i18n/*": ["./app/util/Translation/*"],
      "Api/*": ["./app/api/*"],
      "Component/*": ["./app/component/*"],
      "Constants/*": ["./app/constants/*"],
      "Hooks/*": ["./app/hooks/*"],
      "Route/*": ["./app/route/*"],
      "Style/*": ["./app/style/*"],
      "Store/*": ["./app/store/*"],
      "Type/*": ["./app/type/*"],
      "Util/*": ["./app/util/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
